<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">angel_qoh_prize_form.prize_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Angel\QoH\Block\Adminhtml\Prize\Edit\BackButton" name="back"/>
			<button class="Angel\QoH\Block\Adminhtml\Prize\Edit\DeleteButton" name="delete"/>
			<button class="Angel\QoH\Block\Adminhtml\Prize\Edit\SaveButton" name="save"/>
			<button class="Angel\QoH\Block\Adminhtml\Prize\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>angel_qoh_prize_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>angel_qoh_prize_form.prize_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="prize_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Angel\QoH\Model\Prize\DataProvider" name="prize_form_data_source">
			<settings>
				<requestFieldName>prize_id</requestFieldName>
				<primaryFieldName>prize_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label/>
		</settings>
		<field formElement="input" name="product_id" sortOrder="100">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">product_id</label>
				<dataScope>product_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="picked_number" sortOrder="110">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">picked_number</label>
				<dataScope>picked_number</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="ticket_id" sortOrder="120">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">ticket_id</label>
				<dataScope>ticket_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="card" sortOrder="130">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">card</label>
				<dataScope>card</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="prize" sortOrder="140">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">prize</label>
				<dataScope>prize</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="start_at" sortOrder="150">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">start_at</label>
				<dataScope>start_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="end_at" sortOrder="160">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">end_at</label>
				<dataScope>end_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="status" sortOrder="180">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Prize</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
