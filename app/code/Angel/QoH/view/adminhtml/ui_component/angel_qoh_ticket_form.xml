<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">angel_qoh_ticket_form.ticket_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Angel\QoH\Block\Adminhtml\Ticket\Edit\BackButton" name="back"/>
			<button class="Angel\QoH\Block\Adminhtml\Ticket\Edit\DeleteButton" name="delete"/>
			<button class="Angel\QoH\Block\Adminhtml\Ticket\Edit\SaveButton" name="save"/>
			<button class="Angel\QoH\Block\Adminhtml\Ticket\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>angel_qoh_ticket_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>angel_qoh_ticket_form.ticket_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="ticket_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Angel\QoH\Model\Ticket\DataProvider" name="ticket_form_data_source">
			<settings>
				<requestFieldName>ticket_id</requestFieldName>
				<primaryFieldName>ticket_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label/>
		</settings>
		<field formElement="input" name="product_id" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">product_id</label>
				<dataScope>product_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="customer_id" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">customer_id</label>
				<dataScope>customer_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="start" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">start</label>
				<dataScope>start</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="end" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">end</label>
				<dataScope>end</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="price" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">price</label>
				<dataScope>price</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">true</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="card_number" sortOrder="80">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">card_number</label>
				<dataScope>card_number</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="status" sortOrder="90">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Ticket</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">status</label>
				<dataScope>status</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
