<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">angel_qoh_ticket_listing.angel_qoh_ticket_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>angel_qoh_ticket_columns</spinner>
		<deps>
			<dep>angel_qoh_ticket_listing.angel_qoh_ticket_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Ticket</label>
			</button>
		</buttons>
	</settings>
	<dataSource component="Magento_Ui/js/grid/provider" name="angel_qoh_ticket_listing_data_source">
		<settings>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Angel_QoH::Ticket</aclResource>
		<dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="angel_qoh_ticket_listing_data_source">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>ticket_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>false</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="angel_qoh_ticket_columns">
		<settings>
			<editorConfig>
				<param name="selectProvider" xsi:type="string">angel_qoh_ticket_listing.angel_qoh_ticket_listing.angel_qoh_ticket_columns.ids</param>
				<param name="enabled" xsi:type="boolean">true</param>
				<param name="indexField" xsi:type="string">ticket_id</param>
				<param name="clientConfig" xsi:type="array">
					<item name="saveUrl" path="angel_qoh/Ticket/inlineEdit" xsi:type="url"/>
					<item name="validateBeforeSave" xsi:type="boolean">false</item>
				</param>
			</editorConfig>
			<childDefaults>
				<param name="fieldAction" xsi:type="array">
					<item name="provider" xsi:type="string">angel_qoh_ticket_listing.angel_qoh_ticket_listing.angel_qoh_ticket_columns_editor</item>
					<item name="target" xsi:type="string">startEdit</item>
					<item name="params" xsi:type="array">
						<item name="0" xsi:type="string">${ $.$data.rowIndex }</item>
						<item name="1" xsi:type="boolean">true</item>
					</item>
				</param>
			</childDefaults>
		</settings>
		<selectionsColumn name="ids">
			<settings>
				<indexField>ticket_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="ticket_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="product_id">
			<settings>
				<filter>text</filter>
				<label translate="true">product_id</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">true</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<actionsColumn class="Angel\QoH\Ui\Component\Listing\Column\TicketActions" name="actions">
			<settings>
				<indexField>ticket_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
		<column name="customer_id">
			<settings>
				<filter>text</filter>
				<label translate="true">customer_id</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">true</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="start">
			<settings>
				<filter>text</filter>
				<label translate="true">start</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">true</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="end">
			<settings>
				<filter>text</filter>
				<label translate="true">end</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">true</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="price">
			<settings>
				<filter>text</filter>
				<label translate="true">price</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">true</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="created_at">
			<settings>
				<filter>text</filter>
				<label translate="true">created_at</label>
			</settings>
		</column>
		<column name="credit_transaction_id">
			<settings>
				<filter>text</filter>
				<label translate="true">credit_transaction_id</label>
			</settings>
		</column>
		<column name="card_number">
			<settings>
				<filter>text</filter>
				<label translate="true">card_number</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
		<column name="status">
			<settings>
				<filter>text</filter>
				<label translate="true">status</label>
				<editor>
					<editorType>text</editorType>
					<validation>
						<rule name="required-entry" xsi:type="boolean">false</rule>
					</validation>
				</editor>
			</settings>
		</column>
	</columns>
</listing>
