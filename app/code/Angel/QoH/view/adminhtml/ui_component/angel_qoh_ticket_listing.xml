<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">angel_qoh_ticket_listing.angel_qoh_ticket_listing_data_source</item>
		</item>
	</argument>
	<settings>
		<spinner>angel_qoh_ticket_columns</spinner>
		<deps>
			<dep>angel_qoh_ticket_listing.angel_qoh_ticket_listing_data_source</dep>
		</deps>
		<buttons>
			<button name="add">
				<url path="*/*/new"/>
				<class>primary</class>
				<label translate="true">Add new Ticket</label>
			</button>
			<button name="import">
				<url path="*/*/import"/>
				<class>primary</class>
				<label translate="true">Import Tickets</label>
			</button>
			<button name="print">
				<url path="*/*/printForm"/>
				<class>primary</class>
				<label translate="true">Print Tickets</label>
			</button>
		</buttons>
	</settings>
	<dataSource component="Magento_Ui/js/grid/provider" name="angel_qoh_ticket_listing_data_source">
		<settings>
			<updateUrl path="mui/index/render"/>
		</settings>
		<aclResource>Angel_QoH::Ticket</aclResource>
		<dataProvider class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider" name="angel_qoh_ticket_listing_data_source">
			<settings>
				<requestFieldName>id</requestFieldName>
				<primaryFieldName>ticket_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<listingToolbar name="listing_top">
		<settings>
			<sticky>false</sticky>
		</settings>
		<bookmark name="bookmarks"/>
		<columnsControls name="columns_controls"/>
		<filters name="listing_filters"/>
		<paging name="listing_paging"/>
	</listingToolbar>
	<columns name="angel_qoh_ticket_columns">
		<selectionsColumn name="ids">
			<settings>
				<indexField>ticket_id</indexField>
			</settings>
		</selectionsColumn>
		<column name="ticket_id">
			<settings>
				<filter>text</filter>
				<sorting>asc</sorting>
				<label translate="true">ID</label>
			</settings>
		</column>
		<column name="product_name">
			<settings>
				<filter>text</filter>
				<label translate="true">Product</label>
			</settings>
		</column>
		<column name="customer_email">
			<settings>
				<filter>text</filter>
				<label translate="true">Customer</label>
			</settings>
		</column>
		<column name="start">
			<settings>
				<filter>textRange</filter>
				<label translate="true">Start</label>
			</settings>
		</column>
		<column name="end">
			<settings>
				<filter>textRange</filter>
				<label translate="true">End</label>
			</settings>
		</column>
		<column name="price" class="Magento\Catalog\Ui\Component\Listing\Columns\Price">
			<settings>
				<filter>textRange</filter>
				<label translate="true">Ticket Price</label>
			</settings>
		</column>
		<column name="created_at" class="Magento\Ui\Component\Listing\Columns\Date" component="Magento_Ui/js/grid/columns/date">
			<settings>
				<filter>dateRange</filter>
				<label translate="true">Created At</label>
			</settings>
		</column>
		<column name="card_number">
			<settings>
				<filter>text</filter>
				<label translate="true">Card Number</label>
			</settings>
		</column>
		<column name="status" component="Magento_Ui/js/grid/columns/select">
			<settings>
				<options class="Angel\QoH\Model\Ticket\Status"/>
				<filter>select</filter>
				<label translate="true">Status</label>
				<dataType>select</dataType>
			</settings>
		</column>
		<actionsColumn class="Angel\QoH\Ui\Component\Listing\Column\TicketActions" name="actions">
			<settings>
				<indexField>ticket_id</indexField>
				<resizeEnabled>false</resizeEnabled>
				<resizeDefaultWidth>107</resizeDefaultWidth>
			</settings>
		</actionsColumn>
	</columns>
</listing>
